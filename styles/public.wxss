/* 自定义radio勾选框样式 */
.custom-radio {
  width: 32rpx;
  height: 32rpx;
  box-sizing: border-box;
}

.custom-radio .wx-radio-input {
  width: 32rpx;
  height: 32rpx;
  border-radius: 50%;
  /*圆角*/
}

.custom-radio .wx-radio-input.wx-radio-input-checked::before {
  width: 32rpx;
  height: 32rpx;
  border-radius: 50%;
  line-height: 32rpx;
  text-align: center;
  font-size: 20rpx;
  /* 勾大小 */
  font-weight: bold;
  color: #646464;
  transform: translate(-50%, -50%) scale(1);
}

.custom-radio .wx-radio-input.wx-radio-input-checked {
  border-color: #FFCB57;
  background: #FFCB57;
}

